{
  "private": true,
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.1.0"
  },
  "type": "module",
  "scripts": {
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{js,cjs,mjs}": "./Workbench/build-aux/fun workbench-cli format",
    "*.css": "./Workbench/build-aux/fun workbench-cli format",
    "*.py": "./Workbench/build-aux/fun black",
    "*.rs": "./Workbench/build-aux/fun rustfmt --edition 2021",
    "*.blp": "./build-aux/fun workbench-cli format",
    "*.vala": "./build-aux/fun workbench-cli format vala"
  }
}
